<div class="terminal-container">
  <%= terminal_tag do %>
    <% @build.jobs.each do |job| %>
      <%= job_container @current_tab_name, job do %>
        <%= ascii_job_heading(job.order_index) %>
        <% if job.system_logs.present? %>
          <%= ansi_to_html(job.system_logs) %>
        <% else %>
          Nothing here yet<br>
        <% end %>
      <% end %>
    <% end %>
  <% end %>
</div>
